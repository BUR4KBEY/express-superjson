    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/ui": "3.0.8",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unicorn": "^57.0.0",
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
